<html>
<head>
    <%^head.html%>
    <title>Main</title>
    <script type="text/javascript">$(mainEntry);</script>
</head>
<body>
    <div class="div_title_bar">
        <div class="div_delete_file visit_label">编辑</div>
        <div class="div_add_file visit_label">添加文件</div>
        <div class="div_log_off visit_label">退出</div>
    </div>
    <div class="div_file_panel">
    <%es for (var key in PageInfo.files) {%>
        <div class="div_file_item <%=PageInfo.isMaster?"div_file_item_pc":""%>" key="<%=key%>">
            <div class="div_view_tag"><%=PageInfo.files[key].dispName%></div>
            <input class="input_edit_tag" />
            <%es if (PageInfo.isMaster) {%>
                <div class="div_delete_tag">删除</div>
            <%es }%>
            <div class="div_rename_tag">改名</div>
        </div>
    <%es }%>
    </div>
    <%es if (PageInfo.isMaster) {%>
        <div class="div_bottom_bar">
            <a>序列号:<%=PageInfo.serial%></a>
        </div>
    <%es }%>
</body>
</html>
