<html>
<head>
    <meta charset="utf-8">
    <title>List</title>
    <link rel="stylesheet" type="text/css" href="<%=PageInfo.urlRoot%>/style.css">
    <script type="text/javascript" src="<%=PageInfo.urlRoot%>/jquery.js"></script>
    <script type="text/javascript" src="<%=PageInfo.urlRoot%>/load.js"></script>
    <script type="text/javascript">setRoot("<%=PageInfo.urlRoot%>");</script>
</head>
<body>
<div class="div_title_bar">
    <a href="javascript:choosefile()" class="a_add_file">添加文件</a>
    <a href="javascript:logoff()" class="a_log_off">下线</a>
</div>

<%es for (var key in PageInfo.files) {%>
    <div class="div_file_item">
        <a href="/view?key=<%=key%>" class="a_view_tag"><%=PageInfo.files[key].dispName%></a>
        <%es if (PageInfo.isMaster) {%>
            <a href="javascript:deletefile('<%=key%>')" class="a_delete_tag">删除</a>
        <%es }%>
    </div>
<%es }%>

<%es if (PageInfo.isMaster) {%>
<div class="div_bottom_bar">
    <a>序列号: <%=PageInfo.serial%></a>
</div>
<%es }%>
</body>
</html>
